<h1>APIs example</h1>
<style>
#div1, #div2 {
    float: left;
    width: 70px;
    height: 50px;
    margin: 10px;
    padding: 10px;
    border: 1px solid black;
}
</style>
<script>
function allowDrop(ev) {
    ev.preventDefault();
}

function drag(ev) {
    ev.dataTransfer.setData("text", ev.target.id);
}

function drop(ev) {
    ev.preventDefault();
    var data = ev.dataTransfer.getData("text");
    ev.target.appendChild(document.getElementById(data));
}
</script>
</head>
<body>

<h2>Drag and Drop</h2>
<p>Drag the picture forth and back between the two boxes.</p>

<div id="div1" ondrop="drop(event)" ondragover="allowDrop(event)">
  <img src="/Users/enganga/Desktop/IMG_20170417_122125.jpg" draggable="true" ondragstart="drag(event)" id="drag1" width="88" height="31">
</div>

<div id="div2" ondrop="drop(event)" ondragover="allowDrop(event)"></div>



